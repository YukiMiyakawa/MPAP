      <div>
      <% tweets.each do |tweet| %>
          <p><%= tweet.user.name %></p>
          <p><%= tweet.comment %></p>
          <p>practice_time:<%= tweet.practice_time %>min</p>
          <p>
            <%= link_to '編集', edit_user_tweet_path(user, tweet) %>
            <%= link_to '削除', user_tweet_path(user, tweet), method: :delete %>
          </p>
      <% end %>
      </div>