<div>
<%= form_with model:[user, tweet], local:true do |f| %>
  <%= f.label :Tweet %>
  <%= f.text_area :comment, class: "form-control name" %>

  <%= f.label :'PracticeTime(min)' %>
  <%= f.text_field :practice_time, class: "form-control introduction" %>

  <%= f.hidden_field :start_time, :value => Time.now, class: "form-control start_time" %>

  <%= f.submit class: 'btn btn-secondary' %>
<% end %>
</div>